(define (problem simplestriker)
(:domain robocupdeterministic)
(:objects robot1 ball - movable strikercurrentposition ballcurrentposition kickingposition goaltarget rightflank leftflank rightareaentrypoint leftareaentrypoint middleareaentrypoint - location)
(:init 
    (isat robot1 strikercurrentposition) 
    (isat ball ballcurrentposition) 
    (isrobot robot1) 
    (isball ball)
    (precedes leftflank leftareaentrypoint)
    (precedes leftflank rightareaentrypoint)
    (precedes leftflank middleareaentrypoint)
    (precedes rightflank leftareaentrypoint)
    (precedes rightflank rightareaentrypoint)
    (precedes rightflank middleareaentrypoint)

    (precedes leftflank kickingposition)
    (precedes rightflank kickingposition)
    (precedes leftareaentrypoint kickingposition)
    (precedes rightareaentrypoint kickingposition)
    (precedes middleareaentrypoint kickingposition)

    (precedes leftflank goaltarget)
    (precedes rightflank goaltarget)
    (precedes leftareaentrypoint goaltarget)
    (precedes rightareaentrypoint goaltarget)
    (precedes middleareaentrypoint goaltarget)

    (precedes kickingposition goaltarget)

    (initialposition strikercurrentposition)
    (initialposition ballcurrentposition)
)
(:goal (isat ball goaltarget))
)